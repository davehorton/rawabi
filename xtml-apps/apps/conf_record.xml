            <result name="SUCCESS" link="12" stubbed="0" >'Result'  == 'Success'
AND f_intReturnCode == s_intSUCCESS</result>
          </results>
          <scripts >
            <script language="javascript" timing="first" ><![CDATA[Session.f_intReturnCode = -50;

Server.logInfo("INFO: ********** JAVA CALLING insertRecordEvent *********");
Server.logInfo("INFO: SESSION ID = " + Session._sessionId);
Server.logInfo("INFO: RECORDED FILE LOCATION = " + Session.f_strRecordedFileLocation);
