{
	Server.logError("ERROR: FAIL TO INSERT RECORD EVENT");
	Server.logError("ERROR: RETURN CODE: " + Session.f_intReturnCode);
	Server.logError("ERROR: EXIT ON: " + Result.name);
}]]></script>
