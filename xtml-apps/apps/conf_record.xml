var currentDate = new Date();
Session.f_lRecordStartTime = currentDate.getTime();
Server.logInfo("INFO: RECORDED START TIME  = " + Session.f_lRecordStartTime);]]></script>
            <script language="javascript" timing="last" ><![CDATA[if (Result.id != 2)
{
	Server.logError("ERROR: NOT ABLE TO START RECORD, EXIT ON: " + Result.name);
	Server.logError("ERROR: RETURN CODE: " + Session.f_intReturnCode);
	Server.logError("ERROR: MEDIA SERVER TYPE: " + Session.g_oMS.strType);
	Server.logError("ERROR: EndPoint: " + Session.g_oMS.strEndPoint);
	Server.logError("ERROR: CallId: " + Session.g_oMS.strCallId);
	Server.logError("EROOR: ConnectionId: " + Session.g_oMS.strConnectionId);
}]]></script>
          </scripts>
        </action>
