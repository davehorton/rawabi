      <actions >
        <action id="1" plug-in="Pactolus.MGCPCreate.1" ><?xtml-editor x=165 y=210 ?>
          <!--on PCS ms-->
          <crcx callid="g_oMS.strCallId" remote-sdp="" mode="inactive" capability="2" returns="f_intReturnCode" endpoint="g_oMS.strEndPoint" connection-id="g_oMS.strConnectionId" local-sdp="g_oMS.strContent" packetization-period="g_oMS.intPacketizationPeriod" codec="0" timeout="s_intTIMEOUT" local-connection-options="" second-endpoint-id="" telephone-events="0" ms-type="g_oMS.strType" >
            <parameters >
              <quarantine step="0" loop="0" process="0" discard="0"/>
              <events ><![CDATA[]]></events>
              <signals ><![CDATA[]]></signals>
              <digit-map ><![CDATA[]]></digit-map>
            </parameters>
          </crcx>
          <results >
            <result name="Default" link="11" stubbed="0"/>
            <result name="Success" link="9" stubbed="0"/>
            <result name="Error"/>
            <result name="Timeout"/>
          </results>
          <scripts >
            <script language="javascript" timing="first" ><![CDATA[Session.g_strRecordedSessionId = Session._sessionId;
Session.g_oMS = js_initMS(Session.g_oMS, Session.s_strConfMSType);
Session.g_oConference = js_initMS(Session.g_oConference, Session.s_strConfMSType);

Server.logInfo("RECEIVE START RECORD");
Server.logInfo("CONF CONTROLLER IP IS: " + Session.strConfControllerIP);
Server.logInfo("CONF CONTROLLER PORT IS: " + Session.intConfControllerPort);
Server.logInfo("TRANSACTION ID IS: " + Session.strTransactionId);
Server.logInfo("CONFERENCE ID IS: " + Session.strConferenceId);
Server.logInfo("CONFERENCE CONF SESSION ID IS: " + Session.strConferenceSessionId);
Server.logInfo("REQUESTED SESSION ID IS: " + Session.strRequestorSessionId);
Session.g_oConference.strConferenceId = Session.strConferenceId;
Session.g_strConfControllerIP = Session.strConfControllerIP;
Session.g_intConfControllerPort = Session.intConfControllerPort;

Session.g_oMS.intPacketizationPeriod = Session.s_intPacketizationPeriod;]]></script>
            <script language="javascript" timing="last" ><![CDATA[if (2 == Result.id)
{
	Session.g_oMS.bCurrentlyConnected = true;
}
else
{
	Server.logError("ERROR: NOT ABLE TO CREATE AN ENDPOINT, EXIT ON: " + Result.name);
	Server.logError("ERROR: RETURN CODE: " + Session.f_intReturnCode);
	Server.logError("ERROR: MEDIA SERVER TYPE: " + Session.g_oMS.strType);
}]]></script>
          </scripts>
        </action>
        <action id="2" plug-in="Standard.EndSession.1" ><?xtml-editor x=698 y=136 ?></action>
