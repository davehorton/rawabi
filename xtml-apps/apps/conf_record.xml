      <local-vars >
        <var name="f_intReturnCode" type="i4" >-99</var>
        <var name="f_lRecordStopTime" type="i8" >0</var>
      </local-vars>
      <actions >
        <action id="1" plug-in="Pactolus.MGCPRecord.1" ><?xtml-editor x=117 y=288 ?>
          <!--stop record conference-->
          <record op-mode="1" connection-id="g_oMS.strConnectionId" endpoint="g_oMS.strEndPoint" callid="g_oMS.strCallId" timeout="5" returns="f_intReturnCode" return-immediate="0" ms-type="g_oMS.strType" rid="" eik="" rlt="300" prt="" pst="" rl="" dc="" rc="" id-cleanup="0" play-beep="0" ef="&quot;PCM8M16&quot;" agc="0" agcta="" ga="" sla="" bpf="" bpa="" bpd=""/>
